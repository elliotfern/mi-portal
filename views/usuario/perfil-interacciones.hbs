<div class="container">

  <h1>Mis usuarios favoritos</h1>

  <div class="row g-3">
    {{#each this.dataUsuarios.favoritos}}
    <div class="col">
      <div>
        <h4>{{this.nombreCompleto}}</h4>
        <a href="/usuario/{{this._id}}/detalles"><img src="{{this.imagen}}" alt="" width="200px" /></a>
        <p><a href="/usuario/{{this._id}}/detalles">Ver perfil</a></p>
        <hr />
      </div>
    </div>
    {{/each}}
  </div>

</div>